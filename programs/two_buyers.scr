type <str> as Message;
type <float> as Price;

global protocol BuyerSeller(role B1, role B2, role Seller) {
    Prompt(Message) from B1 to Seller;
    Answer(Message) from Seller to B1;
    Prompt(Message) from Seller to B2;
    Quotation(Price) from B1 to B2;

    choice from B2 to Seller {
        Response(Message) from B2 to Seller;
        End;
    } or {
        End;
    }
}